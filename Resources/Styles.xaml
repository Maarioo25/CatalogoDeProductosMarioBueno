<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--#region Colores Dark-->
    <SolidColorBrush x:Key="Background_Primario_Dark" Color="#202020"/>
    <SolidColorBrush x:Key="Background_Secundario_Dark" Color="#282424"/>
    <SolidColorBrush x:Key="Foreground_Primario_Dark" Color="White"/>
    <SolidColorBrush x:Key="Foreground_Secundario_Dark" Color="DarkGray"/>
    <!--#endregion-->
    
    <!--#region Colores Light-->
    <SolidColorBrush x:Key="Background_Primario_Light" Color="#f1f1f1"/>
    <SolidColorBrush x:Key="Background_Secundario_Light" Color="#ffffff"/>
    <SolidColorBrush x:Key="Foreground_Primario_Light" Color="Black"/>
    <SolidColorBrush x:Key="Foreground_Secundario_Light" Color="DarkGray"/>
    <!--#endregion-->
    
    <!--#region Fuentes-->
    <FontFamily x:Key="Fuente_Primaria">Bahnschrift</FontFamily>
    <FontFamily x:Key="Fuente_Secundaria">Arial</FontFamily>
    <!--#endregion-->

</ResourceDictionary>